    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "pino-pretty": "^10.0.0",
    "serve": "^14.2.0"
  },
  "devDependencies": {
    "tap": "^16.1.0"
  }
